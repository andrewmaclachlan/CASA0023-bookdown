@article{maclachlanSubpixelLandcoverClassification2017,
  title = {Subpixel Land-Cover Classification for Improved Urban Area Estimates Using {{Landsat}}},
  author = {MacLachlan, Andrew and Roberts, Gareth and Biggs, Eloise and Boruff, Bryan},
