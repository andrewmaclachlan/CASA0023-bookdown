  doi = {10.2747/1548-1603.49.5.755},
  url = {http://www.tandfonline.com/doi/abs/10.2747/1548-1603.49.5.755 http://www.tandfonline.com/doi/abs/10.2747/1548-1603.49.5.755#.UzAWtK1_veA http://www.tandfonline.com/doi/pdf/10.2747/1548-1603.49.5.755},
  abstract = {A new relative radiometric normalization approach is presented based on the spectral profile shape of hyperspectral data. We calculate the spectral similarity value of pixels at the same location using spectral angle mapping. The cumulative moving average and its differential values are used to determine the appropriate number of pseudo-invariant features automatically. Band-by-band linear regression of the pseudo-invariant features is used to refine the radiometric normalization results iteratively. We tested the algorithm using six Hyperion data subset images. The proposed method yielded stable results with similar or better performance than other methods for all test sites, when assessed by visual inspection and quantitative analysis.}
}

@article{Kim2013,
