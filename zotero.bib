@article{Aydin2010,
  title = {{{GIS}}-Based Environmental Assessment of Wind Energy Systems for Spatial Planning: {{A}} Case Study from {{Western Turkey}}},
  shorttitle = {{{GIS}}-Based Environmental Assessment of Wind Energy},
  author = {Aydin, Nazli Yonca and Kentel, Elcin and Duzgun, Sebnem},
