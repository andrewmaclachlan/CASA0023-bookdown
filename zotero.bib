@article{Abutaleb2015,
  title = {Assessment of Urban Heat Island Using Remotely Sensed Imagery over Greater Cairo , Egypt},
  author = {Abutaleb, Khaled and Ngie, Adeline and Darwish, Ahmed and Ahmed, Mahmoud and Arafat, Sayed and Ahmed, Fathi},
  date = {2015},
  journaltitle = {Advances in Remote Sensing},
  volume = {4},
  pages = {35--47},
  doi = {10.4236/ars.2015.41004},
  abstract = {The Urban Heat Island (UHI) results in significant and sometimes dramatic increases in air temperature differences between the urban environment and its surrounding areas. The heat island structure may extend from the ground to the top of roofs and canopy levels above ground. The Urban Heat Island effect is a leading factor in a long list of human health problems which are expected to increase with the rapid growth of urban populations and projected future climate change. Urban heat island studies can be conducted through either direct measurements of air temperature using automobile transects and weather station networks or through measuring surface temperature using airborne or satellite remote sensing. In this study, UHI was investigated over the Greater Cairo during both summer and winter seasons over two different dates. Landsat 7 ETM+ data were used and the mono-window algorithm was applied in the study. Results emphasis that both types of heat islands exist in the study area...},
  issue = {March},
  keywords = {"UHI,Greate,greater cairo,landsat,Landsat,mono-window,Mono-Window,remote sensing,Remote Sensing,uhi},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\2K4G6NN4\\Abutaleb et al. - 2015 - Assessment of Urban Heat Island Using Remotely Sensed Imagery over Greater Cairo , Egypt.pdf}
}

