@article{Thiru2003,
  title = {Systematic Review of Scope and Quality of Electronic Patient Record Data in Primary Care},
  author = {Thiru, Krish and Hassey, Alan and Sullivan, Frank},
  date = {2003-05},
  journaltitle = {BMJ (Clinical Research Ed.)},
  shortjournal = {BMJ},
  volume = {326},
  number = {7398},
