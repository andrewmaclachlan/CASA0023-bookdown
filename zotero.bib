@article{Antonarakis2011,
  title = {Evaluating Forest Biometrics Obtained from Ground Lidar in Complex Riparian Forests},
  author = {Antonarakis, Alexander S.},
  date = {2011},
  journaltitle = {Remote Sensing Letters},
  volume = {2},
  number = {1},
  pages = {61--70},
  issn = {2150-704X},
  doi = {10.1080/01431161.2010.493899},
  url = {http://www.tandfonline.com/doi/abs/10.1080/01431161.2010.493899 http://www.tandfonline.com/doi/abs/10.1080/01431161.2010.493899#.UtkH62R_veA http://www.tandfonline.com/doi/pdf/10.1080/01431161.2010.493899},
  abstract = {Terrestrial laser scanning is a technique that has been used increasingly in extracting forest biometrics such as trunk diameter and tree heights. Its potential, however, has not been fully explored in complex forested ecosystems, especially in riparian forests, considered among the most dynamic and complex portions of the Earth's biosphere. In this study, forest inventory data and multiple ground scans were obtained in a sparse managed and dense natural riparian forest on the immediate banks of the mid-lower portion of the Garonne River in Southern France, dominated by black poplar (Populus nigra) and commercial hybrid poplars (Populus √ó euramericana). Overall, the ground-based laser-scanning analysis successfully estimated trunk diameters, tree heights and crown radii from both managed and natural riparian forests. However, the ground scanner analysis was not as successful in identifying all of the trunks in the dense natural riparian forest, with only 141 trunks identified from a total of 234. This also results in allometric scaling exponents for ground scanning, which are significantly different from field-derived exponents. This study thus shows that there may be a limit to the number of trees detected in higher density forests, even with multiple scans.}
}

@article{Appleyard1995,
  title = {The Impact of Urban Development on Recharge and Groundwater Quality in a Coastal Aquifer near Perth, Western Australia},
  author = {Appleyard, S.},
  date = {1995},
  journaltitle = {Hydrogeology journal},
  volume = {3},
  number = {2},
  pages = {65--75},
  issn = {1431-2174},
  doi = {10.1007/s100400050072},
  abstract = {AbstractNear Perth, Western Australia, thirty boreholes were drilled at eleven sites in similar hydrogeological settings. The sites represent three levels of urban development: an uncleared area, a new residential area (less than 20 years old) and an old residential area (more than 60 years old). The objective was to assess the impact of sewered urban development on recharge and groundwater quality. Tritium data suggest that a greater proportion of recharge in the urban areas is from point sources, and this probably caused high redox potentials (up to 291 mV) in groundwater in the old residential area. Recharge in the new urban area is estimated to be about 37 percent of the average annual rainfall of about 0.8 m, and this value is probably greater than recharge in undeveloped areas. Concentrations of sulphate increase progressively with the age of urban development, from an average of 8 mg/L to 69 mg/L, probably as a result of fertilizer input and the oxidation of soil-held sulphides with land clearing. Nitrate concentrations are significantly higher in urban areas (average concentration ¬ø1 mg/L as N) than in the non-urban areas (average concentration ¬°0.5 mg/L as N). However, the measured concentrations are all lower than expected from current fertilizer inputs in sewered urban areas of about 80 kg/ha, indicating that denitrification is taking place. A limited reservoir of reducing agents may be available from denitrification, and this may have implications for the sustained use of groundwater as a potable supply in coastal suburbs of Perth. R\&Eacute;SUM\&Eacute;: Pr\&egrave;s de Perth, en Australie occidentale, 30 forages ont \&eacute;t\&eacute; r\&eacute;alis\&eacute;s en 11 sites dans les m\&ecirc;mes conditions hydrog\&eacute;ologiques. Ces sites pr\&eacute;sentent trois degr\&eacute;s de d\&eacute;veloppement urbain: une zone non am\&eacute;nag\&eacute;e, une zone r\&eacute;sidentielle nouvelle (de moins de 20 ans), une zone r\&eacute;sidentielle ancienne (de plus de 60 ans). L'objectif \&eacute;tait de d\&eacute;terminer l'impact de l'assainissement urbain sur la recharge et sur la qualit\&eacute; des eaux souterraines. Les donn\&eacute;es de tritium indiquent qu'une recharge plus importante se produit ponctuellement dans les zones construites, ce qui est probablement responsable des potentiels redox \&eacute;lev\&eacute;s (jusqu'\&agrave; 291 mV) dans l'eau souterraine \&agrave; long temps de s\&eacute;jour. La recharge dans la zone nouvellement urbanis\&eacute;e est d'environ 37\% des pr\&eacute;cipitations moyennes annuelles (800 mm); cette valeur parait sup\&eacute;rieure \&agrave; celle de la recharge dans les zones non urbanis\&eacute;es. Les concentrations en sulfates augmentent progressivement avec l'\&acirc;ge du d\&eacute;veloppement urbain, de 8 \&agrave; 69 mg/l en moyenne, probablement \&agrave; cause des apports d'engrais et de l'oxydation des sulfures lors du lessivage des sols. Les teneurs en nitrates sont significativement plus fortes en zones urbanis\&eacute;es (concentration moyenne sup\&eacute;rieure \&agrave; 1 mg/l en N) qu'en zones non urbanis\&eacute;es (concentration moyenne inf\&eacute;rieure \&agrave; 0.5 mg/l en N). Ces teneurs restent cependant nettement inf\&eacute;rieurs \&agrave; celles que produiraient, comme on le connait, des apports continus d'engrais de 80 kg/ha en zone urbanis\&eacute;e avec assainissement, ce qui prouve qu'une d\&eacute;nitrification se produit. Un r\&eacute;servoir limit\&eacute; d'agents r\&eacute;ducteurs doit donc intervenir dans la d'\&eacute;nitrification, ce qui peut avoir des implications pour une utilisation durable des eaux souterraines pour l'alimentation en eau potable dans la banlieue c\&ocirc;ti\&egrave;re de Perth. RESUMEN: Cerca de Perth, Australia Occidental, se perforaron treinta sondeos en once localizaciones con similares caracter\&iacute;sticas hidrogeol\&oacute;gicas. Las localizaciones representan tres niveles de desarrollo urbano: un area no desarrollada, un area residencial moderna (menos de veinte a\&ntilde;os de edad) y un \&aacute;rea residencial antigua (m\&aacute;s de 60 a\&ntilde;os). El objetivo era evaluar el impacto del desarrollo en las redes de saneamiento urbanas en la recarga y calidad de las aguas subterr\&aacute;neas. Los datos de tritio sugieren que una gran proporci\&oacute;n de la recarga en \&aacute;reas urbanas procede de focos puntuales; esto es una posible causa de los altos potenciales redox (hasta 291 mV) de las aguas subterr\&aacute;neas en el \&aacute;rea residencial antigua. La recarga en el \&aacute;rea urbana moderna se estima sobre el 37\% de la precipitaci\&oacute;n anual media, de alrededor de 0.8 m, y este valor es probablemente mayor que la recarga en \&aacute;reas no desarrolladas. Las concentraciones de sulfato aumentan progresivamente con la edad del desarrollo urbano, desde una media de 8 mg/l hasta 69 mg/l, probablemente como resultado del aporte de fertilizantes y de la oxidaci\&oacute;n de los sulfuros retenidos durante la explanaci\&oacute;n del terreno. Las concentraciones de nitratos son significativamente m\&aacute;s altas en \&aacute;reas urbanas (concentraci\&oacute;n media ¬ø 1mg/l en forma de N) que en \&aacute;reas no urbanas (concentraci\&oacute;n media ¬° 0.5 mg/l en forma de N). Sin embargo, las concentraciones medidas son todas menores que las esperadas a partir de los valores de aporte de fertilizantes habituales en \&aacute;reas urbanas con red de saneamiento, que son del orden de 80 kg/ha. Esto indica que est\&aacute; teniendo lugar un proceso de desnitrificaci\&oacute;n. Esta desnitrificaci\&oacute;n puede ser posible por la presencia de una acumulaci\&oacute;n limitada de agentes reductores, lo que puede tener implicaciones para el uso sostenido del agua subterr\&aacute;nea como suministro de agua potable en los suburbios costeros de Perth.},
  isbn = {1431-2174},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\ZHTH3IJN\\Appleyard - 1995 - The Impact Of Urban Development On Recharge And Groundwater Quality In A Coastal Aquifer Near Perth, Western Australi.pdf}
