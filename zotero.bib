@article{Patino2013,
  title = {A Review of Regional Science Applications of Satellite Remote Sensing in Urban Settings},
  author = {Patino, Jorge E. and Duque, Juan C.},
  date = {2013},
  journaltitle = {Computers, Environment and Urban Systems},
