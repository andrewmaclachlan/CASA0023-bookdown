@article{Simo2016,
  title = {Landsat and Local Land Surface Temperatures in a Heterogeneous Terrain Compared to {{MODIS}} Values},
  author = {Sim√≥, Gemma and Garc√≠a-Santos, Vicente and Jim√©nez, Maria A. and Mart√≠nez-Villagrasa, Daniel and Picos, Rodrigo and Caselles, Vicente and Cuxart, Joan},
  date = {2016},
  journaltitle = {Remote Sensing},
  volume = {8},
