@article{Quintano2013,
  title = {Multiple {{Endmember Spectral Mixture Analysis}} ({{MESMA}}) to Map Burn Severity Levels from {{Landsat}} Images in {{Mediterranean}} Countries},
  author = {Quintano, Carmen and Fern√°ndez-Manso, Alfonso and Roberts, Dar A.},
