@article{Akbari2003,
  title = {Analyzing the Land Cover of an Urban Environment Using High-Resolution Orthophotos},
  author = {Akbari, Hashem and Rose, Shea and Taha, Haider},
  date = {2003},
  journaltitle = {Landscape and Urban Planning},
  volume = {63},
  number = {1},
  pages = {1--14},
  issn = {01692046},
  doi = {10.1016/S0169-2046(02)00165-2},
  abstract = {To estimate the impact of light-colored surfaces (roofs and pavements) and urban vegetation (trees, grass, shrubs) on meteorology and air quality of a city, it is essential to accurately characterize various urban surfaces. Of particular importance is the characterization of the area fraction of various surface-types as well as the vegetative fraction. In this paper, a method is discussed for developing data on surface-type distribution and city-fabric (land cover) makeup (percentage of various surface-types) using high-resolution orthophtos. We devised a semi-automatic Monte Carlo method to sample the data and visually identify the surface-type for each pixel. The color aerial photographs for Sacramento covered a total of about 65 km2, at 0.3-m resolution. Five major land-use types were examined: (1) downtown and city center, (2) industrial, (3) offices, (4) commercial, and (5) residential. In downtown Sacramento, the top view (above-the-canopy) shows that vegetation covers 30\% of the area, whereas roofs cover 23\% and paved surfaces (roads, parking areas, and sidewalks) 41\%. In the industrial areas, vegetation covers 8-14\% of the area, whereas roofs cover 19-23\%, and paved surfaces 29-44\%. The surface-type percentages in the office area were 21\% trees, 16\% roofs, and 49\% paved areas. In commercial areas, vegetation covers 5-20\%, roofs 19-20\%, paved surfaces 44-68\%. Residential areas exhibit a wide range of percentages among their various surface-types. On average, vegetation covers about 36\% of the area, roofs about 20\%, and paved surfaces about 28\%. Trees mostly shade streets, parking lots, grass, and sidewalks. In most non-residential areas, paved surfaces cover 50-70\% of the under-the-canopy area. In residential areas, on average, paved surfaces cover about 35\% of the area. Land-use/land cover (LULC) data from the United States Geological Survey (USGS) was used to extrapolate these results from neighborhood scales to metropolitan Sacramento. Of an area of roughly 800 km2, defining most urban areas of the metropolitan Sacramento, about half is residential. The total roof area comprises about 150 km2 and the total paved surfaces (roads, parking areas, sidewalks) are about 310 km2. The total vegetated area covers about 230 km2. The remaining 110 km2 consist of barren land and miscellaneous surfaces. ?? 2002 Elsevier Science B.V. All rights reserved.},
  isbn = {0169-2046},
  keywords = {Analyzing high-resolution orthophotos,Urban fabric,Urban LULC},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\WLKTBCQT\\Akbari, Rose, Taha - 2003 - Analyzing the land cover of an urban environment using high-resolution orthophotos.pdf}
}

@article{Al-Taiar2014,
  title = {Short-Term Effect of Dust Storms on the Risk of Mortality Due to Respiratory, Cardiovascular and All-Causes in {{Kuwait}}},
  author = {Al-Taiar, Abdullah and Thalib, Lukman},
  date = {2014-01},
  journaltitle = {International Journal of Biometeorology},
  volume = {58},
  number = {1},
  pages = {69--77},
  issn = {0020-7128, 1432-1254},
  doi = {10.1007/s00484-012-0626-7},
  url = {http://link.springer.com/article/10.1007/s00484-012-0626-7 http://link.springer.com/content/pdf/10.1007/s00484-012-0626-7.pdf},
  abstract = {This study aimed to investigate the impact of dust storms on short-term mortality in Kuwait. We analyzed respiratory and cardiovascular mortality as well as all-cause mortality in relation to dust storm events over a 5-year study period, using data obtained through a population-based retrospective ecological time series study. Dust storm days were identified when the national daily average of PM10 exceeded 200 ùúág/m3. Generalized additive models with Poisson link were used to estimate the relative risk (RR) of age-stratified daily mortality associated with dust events, after adjusting for potential confounders including weather variables and long-term trends. There was no significant association between dust storm events and same-day respiratory mortality (RR = 0.96; 95 \%CI 0.88‚Äì1.04), cardiovascular mortality (RR = 0.98; 95 \%CI 0.96‚Äì1.012) or all-cause mortality (RR = 0.99; 95 \%CI 0.97‚Äì1.00). Overall our findings suggest that local dust, that most likely originates from crustal materials, has little impact on short-term respiratory, cardiovascular or all-cause mortality.},
  langid = {english},
  mendeley-tags = {Animal Physiology,Biophysics and Biological Physics,Cardiovascular,Dust storms,Environment- general,Environmental Health,Kuwait,Mortality,Plant Physiology,Respiratory,meteorology},
  keywords = {Animal Physiology,Biophysics and Biological Physics,Cardiovascular,Dust storms,Environment- general,Environmental Health,Kuwait,meteorology,Mortality,Plant Physiology,Respiratory}
}

