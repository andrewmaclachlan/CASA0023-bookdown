  author = {Lu, Dengsheng and Moran, Emilio and Hetrick, Scott},
  date = {2011-05},
  journaltitle = {ISPRS journal of photogrammetry and remote sensing : official publication of the International Society for Photogrammetry and Remote Sensing (ISPRS)},
