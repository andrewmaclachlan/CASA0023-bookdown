  author = {Zhou, Decheng and Zhang, Liangxia and Hao, Lu and Sun, Ge and Liu, Yongqiang and Zhu, Chao},
  date = {2016},
  journaltitle = {Science of the Total Environment},
  volume = {544},
  number = {219},
  pages = {617--626},
  publisher = {{Elsevier B.V.}},
  issn = {18791026},
  doi = {10.1016/j.scitotenv.2015.11.168},
  url = {http://dx.doi.org/10.1016/j.scitotenv.2015.11.168},
  abstract = {Urban heat island (UHI) represents a major anthropogenic modification to the Earth system and its relationship with urban development is poorly understood at a regional scale. Using Aqua MODIS data and Landsat TM/ETM + images, we examined the spatiotemporal trends of the UHI effect (Œ¥T, relative to the rural reference) along the urban development intensity (UDI) gradient in 32 major Chinese cities from 2003 to 2012. We found that the daytime and nighttime Œ¥T increased significantly (p¬° 0.05, mostly in linear form) along a rising UDI for 27 and 30 out of 32 cities, respectively. More rapid increases were observed in the southeastern and northwestern parts of China in the day and night, respectively. Moreover, the Œ¥T trends differed greatly by season and during daytime in particular. The Œ¥T increased more rapidly in summer than in winter during the day and the reverse occurred at night for most cities. Inter-annually, the Œ¥T increased significantly in about one-third of the cities during both the day and night times from 2003 to 2012, especially in suburban areas (0.25 ¬° UDI ‚â§ 0.5), with insignificant trends being observed for most of the remaining cities. We also found that the Œ¥T patterns along the UDI gradient were largely controlled by local climate-vegetation conditions, while that across years were dominated by human activities. Our results highlight the strong and highly diverse urbanization effects on local climate cross China and offer limitations on how these certain methods should be used to quantify UHI intensity over large areas. Furthermore, the impacts of urbanization on climate are complex, thus future research efforts should focus more toward direct observation and physical-based modeling to make credible predictions of the effects.},
  keywords = {Background climate,Land use activities,MODIS,Spatiotemporal variations,Urban heat island,Urbanization},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\6IYVWCIF\\Zhou et al. - 2016 - Spatiotemporal trends of urban heat island effect along the urban development intensity gradient in China.pdf}
}

