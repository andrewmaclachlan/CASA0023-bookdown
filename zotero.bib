@article{Rodriguez-Galiano2012,
  title = {Random {{Forest}} Classification of {{Mediterranean}} Land Cover Using Multi-Seasonal Imagery and Multi-Seasonal Texture},
  author = {Rodriguez-Galiano, V. F. and Chica-Olmo, M. and Abarca-Hernandez, F. and Atkinson, P. M. and Jeganathan, C.},
