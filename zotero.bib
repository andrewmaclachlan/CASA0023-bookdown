@article{Myneni1997,
  title = {Increased Plant Growth in the Northern High Latitudes from 1981 to 1991},
  author = {Myneni, RB B and Keeling, CD D and Tucker, CJ J and Asrar, G and Nemani, R R},
  date = {1997},
  journaltitle = {Nature},
  volume = {386},
  number = {6626},
  eprint = {73},
  eprinttype = {pmid},
  pages = {698--702},
  issn = {0028-0836},
  doi = {10.1038/386698a0},
  url = {http://ecocast.arc.nasa.gov/pubs/pdfs/1997/Myneni_Nature.pdf%5Cn%3CGo to ISI%3E://A1997WU38700051},
  abstract = {Variations in the amplitude and timing of the seasonal cycle of atmospheric CO2 have shown an association with surface air temperature consistent with the hypothesis that warmer temperatures have promoted increases in plant growth during summer(1) and/or plant respiration during winter(2) in the northern high latitudes. Here we present evidence from satellite data that the photosynthetic activity of terrestrial vegetation increased from 1981 to 1991 in a manner that is suggestive of an increase in plant growth associated with a lengthening of the active growing season. The regions exhibiting the greatest increase lie between 45 degrees N and 70 degrees N, where marked warming has occurred in the spring time(3) due to an early disappearance of snow(4). The satellite data are concordant with an increase in the amplitude of the seasonal cycle of atmospheric carbon dioxide exceeding 20\% since the early 1970s, and an advance of up to seven days in the timing of the drawdown of CO2 in spring and early summer(1). Thus, both the satellite data and the CO2 record indicate that the global carbon cycle has responded to interannual fluctuations in surface air temperature which, although small at the global scale, are regionally highly significant.},
  isbn = {0028-0836},
  keywords = {AVHRR DATA,RISE,VEGETATION INDEX}
}

@article{Myneni2007,
  title = {Large Seasonal Swings in Leaf Area of {{Amazon}} Rainforests},
  author = {Myneni, Ranga B. and Yang, Wenze and Nemani, Ramakrishna R. and Huete, Alfredo R. and Dickinson, Robert E. and Knyazikhin, Yuri and Didan, Kamel and Fu, Rong and Ju√°rez, Robinson I. Negr√≥n and Saatchi, Sasan S. and Hashimoto, Hirofumi and Ichii, Kazuhito and Shabanov, Nikolay V. and Tan, Bin and Ratana, Piyachat and Privette, Jeffrey L. and Morisette, Jeffrey T. and Vermote, Eric F. and Roy, David P. and Wolfe, Robert E. and Friedl, Mark A. and Running, Steven W. and Votava, Petr and El-Saleous, Nazmi and Devadiga, Sadashiva and Su, Yin and Salomonson, Vincent V.},
  date = {2007-03},
  journaltitle = {Proceedings of the National Academy of Sciences},
  volume = {104},
  number = {12},
  pages = {4820--4823},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.0611338104},
  url = {http://www.pnas.org/content/104/12/4820 http://www.ncbi.nlm.nih.gov/pubmed/17360360 http://www.pnas.org/content/104/12/4820.full.pdf},
  abstract = {Despite early speculation to the contrary, all tropical forests studied to date display seasonal variations in the presence of new leaves, flowers, and fruits. Past studies were focused on the timing of phenological events and their cues but not on the accompanying changes in leaf area that regulate vegetation‚Äìatmosphere exchanges of energy, momentum, and mass. Here we report, from analysis of 5 years of recent satellite data, seasonal swings in green leaf area of ‚âà25\% in a majority of the Amazon rainforests. This seasonal cycle is timed to the seasonality of solar radiation in a manner that is suggestive of anticipatory and opportunistic patterns of net leaf flushing during the early to mid part of the light-rich dry season and net leaf abscission during the cloudy wet season. These seasonal swings in leaf area may be critical to initiation of the transition from dry to wet season, seasonal carbon balance between photosynthetic gains and respiratory losses, and litterfall nutrient cycling in moist tropical forests.},
  langid = {english},
  mendeley-tags = {Remote sensing,tropical forests phenology,vegetation climate interaction},
  keywords = {Remote sensing,tropical forests phenology,vegetation climate interaction}
}

