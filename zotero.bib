  author = {Tan, Jianguo and Zheng, Youfei and Tang, Xu and Guo, Changyi and Li, Liping and Song, Guixiang and Zhen, Xinrong and Yuan, Dong and Kalkstein, Adam J. and Li, Furong and Chen, Heng},
