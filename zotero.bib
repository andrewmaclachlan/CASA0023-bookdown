@article{Stephens2014,
  title = {Temperate and Boreal Forest Mega-Fires: Characteristics and Challenges},
  shorttitle = {Temperate and Boreal Forest Mega-Fires},
  author = {Stephens, Scott L and Burrows, Neil and Buyantuyev, Alexander and Gray, Robert W and Keane, Robert E and Kubian, Rick and Liu, Shirong and Seijo, Francisco and Shu, Lifu and Tolhurst, Kevin G and van Wagtendonk, Jan W},
