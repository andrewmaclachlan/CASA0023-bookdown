@article{Ivajnsic2014,
  title = {Geographically Weighted Regression of the Urban Heat Island of a Small City},
  author = {Ivajn≈°iƒç, Danijel and Kaligariƒç, Mitja and ≈Ωiberna, Igor},
  date = {2014},
  journaltitle = {Applied Geography},
  volume = {53},
