  keywords = {Accuracy assessment,Land-cover mapping,Support vector machine},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\2QI4IMNX\\Shao, Lunetta - 2012 - Comparison of support vector machine, neural network, and CART algorithms for the land-cover classification using.pdf}
