@article{Gutman2013,
  title = {Assessment of the {{NASA}}-{{USGS}} Global Land Survey ({{GLS}}) Datasets},
  author = {Gutman, Garik and Huang, Chengquan and Chander, Gyanesh and Noojipady, Praveen and Masek, Jeffrey G.},
