@article{Gastner2004,
  title = {Diffusion-Based Method for Producing Density-Equalizing Maps},
  author = {Gastner, Michael T. and Newman, M. E. J.},
  date = {2004-05},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {101},
  number = {20},
  pages = {7499--7504},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.0400280101},
  url = {http://www.pnas.org/content/101/20/7499 http://www.ncbi.nlm.nih.gov/pubmed/15136719 http://www.pnas.org/content/101/20/7499.full.pdf},
  abstract = {Map makers have for many years searched for a way to construct cartograms, maps in which the sizes of geographic regions such as countries or provinces appear in proportion to their population or some other analogous property. Such maps are invaluable for the representation of census results, election returns, disease incidence, and many other kinds of human data. Unfortunately, to scale regions and still have them fit together, one is normally forced to distort the regions' shapes, potentially resulting in maps that are difficult to read. Many methods for making cartograms have been proposed, some of them are extremely complex, but all suffer either from this lack of readability or from other pathologies, like overlapping regions or strong dependence on the choice of coordinate axes. Here, we present a technique based on ideas borrowed from elementary physics that suffers none of these drawbacks. Our method is conceptually simple and produces useful, elegant, and easily readable maps. We illustrate the method with applications to the results of the 2000 U.S. presidential election, lung cancer cases in the State of New York, and the geographical distribution of stories appearing in the news.},
  langid = {english}
}

@article{Gei√ü2013,
  title = {Remote Sensing Contributing to Assess Earthquake Risk: From a Literature Review towards a Roadmap},
  shorttitle = {Remote Sensing Contributing to Assess Earthquake r},
  author = {Gei√ü, Christian and Taubenb√∂ck, Hannes},
  date = {2013-08},
  journaltitle = {Natural Hazards},
  volume = {68},
  number = {1},
  pages = {7--48},
  issn = {0921-030X, 1573-0840},
  doi = {10.1007/s11069-012-0322-2},
  url = {http://link.springer.com/article/10.1007/s11069-012-0322-2 http://link.springer.com/content/pdf/10.1007/s11069-012-0322-2.pdf},
  abstract = {Remote sensing data and methods are widely deployed in order to contribute to the assessment of numerous components of earthquake risk. While for earthquake hazard-related investigations, the use of remotely sensed data is an established methodological element with a long research tradition, earthquake vulnerability‚Äìcentred assessments incorporating remote sensing data are increasing primarily in recent years. This goes along with a changing perspective of the scientific community which considers the assessment of vulnerability and its constituent elements as a pivotal part of a comprehensive risk analysis. Thereby, the availability of new sensors systems enables an appreciable share of remote sensing first. In this manner, a survey of the interdisciplinary conceptual literature dealing with the scientific perception of risk, hazard and vulnerability reveals the demand for a comprehensive description of earthquake hazards as well as an assessment of the present and future conditions of the elements exposed. A review of earthquake-related remote sensing literature, realized both in a qualitative and quantitative manner, shows the already existing and published manifold capabilities of remote sensing contributing to assess earthquake risk. These include earthquake hazard-related analysis such as detection and measurement of lineaments and surface deformations in pre- and post-event applications. Furthermore, pre-event seismic vulnerability‚Äìcentred assessment of the built and natural environment and damage assessments for post-event applications are presented. Based on the review and the discussion of scientific trends and current research projects, first steps towards a roadmap for remote sensing are drawn, explicitly taking scientific, technical, multi- and transdisciplinary as well as political perspectives into account, which is intended to open possible future research activities.},
  langid = {english},
  mendeley-tags = {Civil Engineering,Earthquake,Environmental Management,Geophysics/Geodesy,Geotechnical Engineering \& Applied Earth Sciences,Hazard,Hydrogeology,Literature review,Natural Hazards,Remote sensing,Risk,Roadmap,Vulnerability},
  keywords = {Civil Engineering,Earthquake,Environmental Management,Geophysics/Geodesy,Geotechnical Engineering & Applied Earth Sciences,Hazard,Hydrogeology,Literature review,Natural Hazards,Remote sensing,Risk,Roadmap,Vulnerability}
}

@article{Gemitzi2006,
