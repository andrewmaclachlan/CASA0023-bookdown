  keywords = {Land surface temperature,Landsat,Multitemporal},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\SFN828YP\\Vlassova et al. - 2014 - Assessment of methods for land surface temperature retrieval from landsat-5 TM images applicable to multiscale.pdf}
