@article{Lucas2008,
  title = {Retrieving Forest Biomass through Integration of {{CASI}} and {{LiDAR}} Data},
  author = {Lucas, R. M. and Lee, A. C. and Bunting, P. J.},
  date = {2008},
  journaltitle = {International Journal of Remote Sensing},
  volume = {29},
  number = {5},
  pages = {1553--1577},
  issn = {0143-1161},
  doi = {10.1080/01431160701736497},
  url = {http://www.tandfonline.com/doi/abs/10.1080/01431160701736497 http://www.tandfonline.com/doi/abs/10.1080/01431160701736497#.Uoimz2ROrFc http://www.tandfonline.com/doi/pdf/10.1080/01431160701736497},
  abstract = {To increase understanding of forest carbon cycles and stocks, estimates of total and component (e.g. leaf, branch and trunk) biomass at a range of scales are desirable. Focusing on mixed species forests in central south‚Äêeast Queensland, two different approaches to the retrieval of biomass from small footprint Light Detection and Ranging (LiDAR) and Compact Airborne Spectrographic Imager (CASI) hyperspectral data were developed and compared. In the first, stems were located using a LiDAR crown openness index, and each was associated with crowns delineated and identified to species using CASI data. The component biomass for individual trees was then estimated using LiDAR‚Äêderived height and stem diameter as input to species‚Äêspecific allometric equations. When summed to give total above‚Äêground biomass (AGB) and aggregated to the plot level, these estimates showed a reasonable correspondence with ground (plot‚Äêbased) estimates (r 2 = 0.56, RSE = 25.3 Mg ha‚àí1, n = 21) given the complex forest being assessed. In the second approach, a Jackknife linear regression utilizing six LiDAR strata heights and crown cover at the plot‚Äêscale produced more robust estimates of AGB that showed a closer correspondence with plot‚Äêscale ground data (r 2 = 0.90, RSE = 11.8 Mg ha‚àí1, n = 31). AGB aggregated from the tree‚Äêlevel and Jackknife regression plot‚Äêbased AGB estimates (for 270 plots‚Äîeach of 0.25 ha) compared well for more mature homogeneous and open forests. However, at the tree level, AGB was overestimated in taller forests dominated by trees with large spreading crowns, and underestimated AGB where an understorey with a high density of stems occurred. The study demonstrated options for quantifying component biomass and AGB through integration of LiDAR and CASI data but highlighted the requirement for methods that give improved estimation of tree density (by size class distributions) and species occurrence in complex forests.}
}

