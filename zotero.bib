@article{Friedl2002,
  title = {Global Land Cover Mapping from {{MODIS}}: Algorithms and Early Results},
  author = {Friedl, M.A and McIver, D.K and Hodges, J.C.F and Zhang, X.Y and Muchoney, D and Strahler, A.H and Woodcock, C.E and Gopal, S and Schneider, A and Cooper, A and Baccini, A and Gao, F and Schaaf, C},
  date = {2002},
  journaltitle = {Remote Sensing of Environment},
  volume = {83},
